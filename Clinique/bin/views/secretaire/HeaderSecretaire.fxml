<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" spacing="10.0" stylesheets="@../../styles/app.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.clinique.controller.DashboardController">
   <children>
      <Button fx:id="btnMedecins" mnemonicParsing="false" onAction="#handleMedecins" styleClass="dashboard-button" text="Médecins" />
      <Button fx:id="btnPatients" mnemonicParsing="false" onAction="#handlePatients" styleClass="dashboard-button" text="Patients" />
      <Button fx:id="btnRendezvous" mnemonicParsing="false" onAction="#handleRendezVous" styleClass="dashboard-button" text="Rendez-vous" />
      <Button fx:id="btnCreerRendezVous" mnemonicParsing="false" onAction="#handleCreerRendezVous" styleClass="button-add" text="Créer un rendez-vous" />
      <Button fx:id="btnLogout" mnemonicParsing="false" onAction="#handleLogout" styleClass="logout-button" text="Déconnexion" HBox.hgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
